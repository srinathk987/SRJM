        $scope.CalculateVatTax = function (NewOrder) {
            debugger;
            var discount = 0;



            var switch_left = document.getElementById('switch_left').checked;
            var switch_right = document.getElementById('switch_right').checked;
            if (switch_left == true) {

                $scope.NEWORDER["discounttype"] = "Per";
            }
            if (switch_right == true) {

                $scope.NEWORDER["discounttype"] = "Flat";
            }
            NewOrder["OrderTotal"] = $scope.Items.reduce(function (pv, cv) {
                debugger;
                return pv + cv.Rate;
            }, 0);


            if (NewOrder.VatTax === undefined || NewOrder.VatTax == "") { NewOrder.VatTax = 0; }
            if (NewOrder.OtherCharges === undefined || NewOrder.OtherCharges == "") { NewOrder.OtherCharges = 0; }
            if (NewOrder.discounttype == "Per") {

                discount = parseFloat((parseInt(NewOrder.OrderTotal)) * parseInt(NewOrder.Discount)) / 100
            }
            else {

                discount = NewOrder.Discount;

            }
            var totalvatcalaamount = (parseFloat(NewOrder.OrderTotal) + parseFloat(NewOrder.OtherCharges) - parseFloat(discount) + parseFloat(NewOrder.servicetaxvalue)) * parseFloat(NewOrder.VatTax) / 100;
            $scope.NEWORDER.VatTaxAmount = totalvatcalaamount.toFixed(2);

            var net = parseFloat(NewOrder.OrderTotal) + parseFloat(NewOrder.OtherCharges) - parseFloat(discount) + parseFloat(NewOrder.servicetaxvalue) + totalvatcalaamount;
            $scope.NetTotal = Math.round(net);
            NewOrder.OrderNetTotal = Math.round($scope.NetTotal);
        }
        $scope.Calculateservicecharge = function (NewOrder) {
            debugger;


            if (NewOrder.OtherCharges === undefined || NewOrder.OtherCharges == "") { NewOrder.OtherCharges = 0; }
            //if (NewOrder.Discount != "") {
            if (NewOrder.discounttype == "Per") {

                if (isNaN(NewOrder.Discount)) {

                    NewOrder.Discount = 0;
                }
                var servicetax = NewOrder.servicetax;
                if (servicetax == "" || servicetax === undefined) { NewOrder.servicetaxvalue = 0; servicetax = 0 }
                var tax = ((parseFloat(NewOrder.OrderTotal)) * parseInt(NewOrder.Discount) / 100)
                var servicetaxcal = (parseFloat(((parseFloat(NewOrder.OrderTotal) + parseFloat(NewOrder.OtherCharges)) - tax) * parseInt(servicetax) / 100));

                NewOrder.servicetaxvalue = (servicetaxcal.toFixed(2));
                var net = (parseInt(NewOrder.OrderTotal) - parseFloat(parseInt(NewOrder.OrderTotal) * parseInt(NewOrder.Discount)) / 100);
                if (net == 0) {
                    net = (parseInt(NewOrder.OrderTotal) - parseFloat(parseInt(NewOrder.OrderTotal) * parseInt(NewOrder.Discount)) / 100);
                    NewOrder.servicetaxvalue = 0;
                }
                else {

                    net = (parseInt(NewOrder.OrderTotal) + parseFloat(NewOrder.OtherCharges) - parseFloat(parseInt(NewOrder.OrderTotal) * parseInt(NewOrder.Discount)) / 100) + parseFloat(servicetaxcal);
                }
                //+parseInt(NewOrder.DeliverCharges)
                $scope.NetTotal = net;
                NewOrder.OrderNetTotal = $scope.NetTotal;
            }

            else if (NewOrder.discounttype == "Flat") {

                var servicetax = NewOrder.servicetax;
                if (servicetax == "" || servicetax === undefined) { NewOrder.servicetaxvalue = 0; servicetax = 0 }
                var servicetaxcal = (parseFloat((parseFloat(NewOrder.OrderTotal) + parseFloat(NewOrder.OtherCharges) - parseInt(NewOrder.Discount)) * parseInt(servicetax) / 100));

                NewOrder.servicetaxvalue = servicetaxcal.toFixed(2);
                var net = parseFloat(NewOrder.OrderTotal) + parseFloat(NewOrder.OtherCharges) - parseInt(NewOrder.Discount) + parseFloat(servicetaxcal);
                $scope.NetTotal = net;
                NewOrder.OrderNetTotal = $scope.NetTotal;
            }
            //}

            $scope.CalculateVatTax(NewOrder);
        }
